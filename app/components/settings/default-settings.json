{
  // Mapping file extension to what they should be displayed at
  "fileTypes": {
    "log": ["txt", "log"],
    "image":  ["png", "jpg", "gif"],
    "code": ["json", "ts2", "js", "java", "cs", "cpp", "h", "hpp", "py", "xml", "sh", "cmat", "bat"]
  },
  // Default view for the configuration tab of pool, job, task, etc. Can be [pretty,json]
  "configuration.default-view": "pretty"
}
