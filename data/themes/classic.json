{
  "$schema": "./theme.schema.json",
  "type": "standard",
  "primary": "#2F71C4",
  "primary-contrast": "#ffffff",
  "danger": "#aa3939",
  "danger-contrast": "#ffffff",
  "warn": "#a46026",
  "warn-contrast": "#ffffff",
  "success": "#118A0F",
  "success-contrast": "#ffffff",
  "main-background": "#f5f5f5",
  "secondary-background": "#e5e5e5",
  "card-background": "white",
  "hover-bg": "#d5d5d5",
  "selection": "var(--color-primary)",
  "outline": "#528bff",
  "border": "#d5d5d5",
  "editor": "vs",
  "text": {
    "primary": "#333333",
    "secondary": "#666666"
  },
  "breadcrumb": {
    "text": "white",
    "background": "#5b5b5b",
    "hover-background": "#666666"
  },
  "header": {
    "text": "white",
    "background": "#333333",
    "hover-background": "#757575"
  },
  "navigation": {
    "text": "white",
    "background": "#5b5b5b",
    "hover-background": "#666666"
  },
  "footer": {
    "text": "whitesmoke",
    "background": "var(--color-primary)",
    "hover": "#757575"
  },
  "file-explorer": {
    "folder-icon": "#f8d979"
  },
  "button": {
    "basic-text": "#2865bf",
    "basic-bg": "#e5e5e5",
    "basic-hover-bg": "#d5d5d5",
    "disabled-text": "#d5d5d5",
    "disabled-bg": "#ededed"
  },
  "monitorChart": {
    "core-count": "#1c3f95",
    "low-priority-core-count": "#ffa90d",
    "task-start-event": "#ffa90d",
    "task-complete-event": "#388e3c",
    "task-fail-event": "#aa3939",
    "starting-node-count": "#1c3f95",
    "idle-node-count": "#be93d9",
    "running-node-count": "#388e3c",
    "start-task-failed-node-count": "#aa3939",
    "rebooting-node-count": "#ff755c"
  },
  "input": {
    "border": "#949494",
    "focus-border": "var(--color-primary)",
    "placeholder": "var(--color-text-secondary)",
    "text": "var(--color-text-primary)",
    "background": "#fff",
    "disabled-border": "#d5d5d5",
    "disabled-text": "var(--color-text-primary)",
    "disabled-background": "#e5e5e5"
  },
  "chart-colors": [
    "#003f5c",
    "#aa3939",
    "#4caf50",
    "#ffa600"
  ]
}
