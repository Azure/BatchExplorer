{
  "$schema": "./theme.schema.json",
  "primary": "#61afef",
  "danger": "#e06c75",
  "warn": "#f4ad42",
  "success": "#4caf50",
  "main-background": "#21252B",
  "secondary-background": "#333842",
  "card-background": "#282c34",
  "hover-bg": "#3d4148",
  "border": "#474a4f",
  "editor": "vs-dark",
  "text": {
    "primary": "#cccccc",
    "secondary": "#a7a8a9"
  },
  "breadcrumb": {
    "text": "white",
    "background": "#5b5b5b",
    "hover-background": "#666666"
  },
  "header": {
    "text": "#9da5b4",
    "background": "#333333",
    "hover-background": "#757575"
  },
  "navigation": {
    "text": "white",
    "background": "#333842",
    "hover-background": "#666666"
  },
  "footer": {
    "text": "whitesmoke",
    "background": "var(--color-primary)",
    "hover": "#757575"
  },
  "button": {
    "basic-text": "var(--color-primary)",
    "basic-bg": "#3F3F46",
    "basic-hover-bg": "#373744",
    "disabled-text": "#8B8B8B",
    "disabled-bg": "#2D2D30"
  },
  "input": {
    "border": "#3C4049",
    "focus-border": "var(--color-primary)",
    "placeholder": "#d5d5d5",
    "text": "var(--color-text-primary)",
    "background": "#21252B",
    "disabled-text": "var(--color-text-primary)",
    "disabled-border": "#3C4049",
    "disabled-background": "#2D2D30"
  }
}
