{
  "name": "batch-labs",
  "description": "Batch labs is a tool to manage your Azure Batch accounts",
  "repository": {
    "type": "git",
    "url": "https://github.com/timotheeguerin/BatchLabs"
  },
  "author": {
    "name": "Microsoft Corporation"
  },
  "version": "0.6.0",
  "main": "build/client/main.prod.js",
  "scripts": {
    "ts": "ts-node --project tsconfig.node.json",
    "start": "npm run -s electron:prod",
    "clean": "rimraf build/* dll/* python/build/*",
    "test": "npm run -s test-client && npm run -s test-app",
    "test-app": "karma start",
    "test-app:mem": "cross-env DEBUG_MEM=1 npm run -s test-app",
    "test-app-watch": "karma start --auto-watch --no-single-run",
    "test-app-watch:time": "cross-env DEBUG_TIME=1 npm run -s test-app-watch",
    "test-browser": "npm run -s test-app",
    "test-browser-watch": "npm run -s test-app-watch",
    "test-client": "cross-env NODE_ENV=test node test/client/run-jasmine.js",
    "test-client-watch": "npm run -s test-client -- --watch",
    "build-client": "tsc -p tsconfig.node.json",
    "build-app": "webpack --profile --colors --display-error-details --display-cached",
    "build-python": "npm run -s ts config/build-python.ts",
    "build": "npm run -s clean && npm run -s build-client && npm run -s build-app",
    "build:prod": "cross-env NODE_ENV=production npm run -s build",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "electron": "electron build/client/main.js",
    "electron:prod": "cross-env NODE_ENV=production electron build/client/main.js",
    "dev": "concurrently --kill-others \"npm run -s dev-server\" \"npm run -s dev-electron\"",
    "dev-electron": "cross-env HOT=1 electron build/client/main.js",
    "dev-server": "webpack-dev-server --hot --inline --colors --content-base app/",
    "tslint": "tslint -c tslint.json \"app/**/*.ts\" \"src/**/*.ts\" \"test/**/*.ts\" \"./*.js\" \"config/**/*.js\"",
    "stylelint": "stylelint --syntax scss \"app/**/*.scss\"",
    "lint": "npm run -s tslint && npm run -s stylelint",
    "package": "build",
    "build-and-pack": "npm run -s build:prod && npm run -s build-python && npm run -s package --dir"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "build": {
    "productName": "BatchLabs",
    "appId": "microsoft.azure.batchlabs",
    "files": [
      "build/",
      "node_modules/",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "python/build/",
        "to": "python-rpc",
        "filter": [
          "**/*"
        ]
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "app/assets/images/icon.icns"
    },
    "win": {
      "icon": "app/assets/images/icon.ico",
      "target": [
        "dir",
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    },
    "publish": {
      "provider": "github"
    }
  },
  "devDependencies": {
    "@types/bunyan": "~1.8.0",
    "@types/chart.js": "^2.4.0",
    "@types/codemirror": "~0.0.40",
    "@types/core-decorators": "^0.10.30",
    "@types/d3": "^4.4.0",
    "@types/electron": "^1.4.31",
    "@types/electron-json-storage": "0.0.19",
    "@types/extract-zip": "~1.6.2",
    "@types/hammerjs": "^2.0.33",
    "@types/inflection": "^1.5.28",
    "@types/jasmine": "~2.5.53",
    "@types/moment": "^2.13.0",
    "@types/moment-duration-format": "1.3.1",
    "@types/node": "~8.0.2",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "^3.2.1",
    "codelyzer": "~3.1.1",
    "concurrently": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "~5.0.1",
    "css-loader": "~0.28.0",
    "devtron": "^1.4.0",
    "electron": "~1.6.11",
    "electron-builder": "^19.24.1",
    "electron-rebuild": "^1.4.0",
    "file-loader": "~0.11.0",
    "html-webpack-plugin": "^2.28.0",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "~2.0.1",
    "merge2": "^1.0.2",
    "node-sass": "~4.5.0",
    "optimize-js-plugin": "0.0.4",
    "raw-loader": "^0.5.1",
    "rimraf": "~2.6.1",
    "sass-loader": "~6.0.2",
    "style-loader": "~0.18.2",
    "stylelint": "~8.0.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.2.0",
    "tslint": "~5.6.0",
    "typescript": "~2.4.2",
    "url-loader": "^0.5.7",
    "webpack": "~3.5.5",
    "webpack-dev-server": "^2.2.0",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "~4.1.0"
  },
  "dependencies": {
    "@angular/animations": "4.3.5",
    "@angular/cdk": "~2.0.0-beta.8",
    "@angular/common": "4.3.5",
    "@angular/compiler": "4.3.5",
    "@angular/compiler-cli": "4.3.5",
    "@angular/core": "4.3.5",
    "@angular/forms": "4.3.5",
    "@angular/http": "4.3.5",
    "@angular/material": "~2.0.0-beta.7",
    "@angular/platform-browser": "4.3.5",
    "@angular/platform-browser-dynamic": "4.3.5",
    "@angular/router": "4.3.5",
    "@angular/tsc-wrapped": "4.3.5",
    "angular-tree-component": "~4.1.0",
    "azure-batch": "~2.1.0-preview",
    "azure-storage": "^2.1.0",
    "bunyan": "^1.8.4",
    "chart.js": "~2.6.0",
    "codemirror": "~5.28.0",
    "core-decorators": "~0.19.0",
    "d3": "~4.10.0",
    "download": "~6.2.5",
    "electron-json-storage": "~3.0.4",
    "element-resize-detector": "^1.1.9",
    "extract-text-webpack-plugin": "~2.1.0",
    "extract-zip": "~1.6.5",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "immutable": "^3.8.1",
    "inflection": "^1.10.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.15.2",
    "moment-duration-format": "^1.3.0",
    "mousetrap": "^1.6.0",
    "reflect-metadata": "^0.1.9",
    "roboto-fontface": "^0.7.0",
    "rxjs": "^5.4.2",
    "stylint": "~1.5.9",
    "zone.js": "0.8.10"
  }
}
