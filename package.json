{
  "name": "batch-labs",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/BatchLabs"
  },
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-client && npm run test-browser",
    "test-browser": "karma start --single-run",
    "test-browser-watch": "karma start --auto-watch",
    "test-client": "cross-env NODE_ENV=test node test/client/run-jasmine.js",
    "test-client-watch": "npm run test-client -- --watch",
    "build-node-ts": "gulp build-node-ts",
    "build-app": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build-node-ts && npm run build-app",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "electron": "electron build/client/main.js",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run dev-electron\"",
    "dev-electron": "cross-env HOT=1 electron build/client/main.js",
    "dev-server": "node_modules/.bin/webpack-dev-server -d --hot --inline --colors --content-base build/",
    "tslint": "tslint -c tslint.json \"app/**/*.ts\" \"client/**/*.ts\" \"test/**/*.ts\"",
    "stylelint": "stylelint --syntax scss \"app/**/*.scss\"",
    "lint": "npm run tslint && npm run stylelint"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@types/bunyan": "0.0.32",
    "@types/core-decorators": "^0.10.30",
    "@types/electron": "^1.4.25",
    "@types/electron-json-storage": "0.0.19",
    "@types/hammerjs": "^2.0.33",
    "@types/jasmine": "^2.5.36",
    "@types/moment": "^2.13.0",
    "@types/moment-duration-format": "^1.3.1",
    "@types/node": "^6.0.46",
    "@types/sinon": "^1.16.31",
    "angular2-template-loader": "^0.6.0",
    "codelyzer": "^2.0.0-beta.1",
    "concurrently": "^3.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "electron": "1.4.6",
    "electron-debug": "^1.0.1",
    "electron-prebuilt": "1.4.6",
    "electron-rebuild": "^1.4.0",
    "gulp": "^3.9.1",
    "gulp-sourcemaps": "2.2.0",
    "gulp-typescript": "^3.1.2",
    "jasmine": "^2.5.2",
    "jasmine-spec-reporter": "^2.7.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-electron": "^5.1.1",
    "karma-electron-launcher": "^0.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "merge2": "^1.0.2",
    "node-sass": "^3.10.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "ts-loader": "1.2.1",
    "ts-node": "2.0.0",
    "tslint": "^4.0.2",
    "typescript": "~2.1.4",
    "vrsource-tslint-rules": "^0.14.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.15.0"
  },
  "dependencies": {
    "@angular/common": "2.3.0",
