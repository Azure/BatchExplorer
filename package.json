    "extract-text-webpack-plugin": "^1.0.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "immutable": "^3.8.1",
    "inflection": "^1.10.0",
    "moment": "^2.15.2",
    "moment-duration-format": "^1.3.0",
    "mousetrap": "^1.6.0",
    "reflect-metadata": "0.1.8",
