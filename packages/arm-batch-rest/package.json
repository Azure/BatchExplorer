{
  "name": "@batch/arm-batch-rest",
  "sdk-type": "mgmt",
  "author": "Microsoft Corporation",
  "version": "1.0.0",
  "description": "A generated Rest Level Client SDK for the Batch Management Plane.",
  "keywords": [
    "node",
    "azure",
    "cloud",
    "typescript",
    "browser",
    "isomorphic"
  ],
  "license": "MIT",
  "main": "./lib-cjs/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/BatchExplorer"
  },
  "files": [
    "lib",
    "lib-cjs"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "test": "jest",
  "scripts": {
    "build": "npm run compile",
    "build:clean": "run-s clean compile",
    "build:test": "run-s build test",
    "compile": "run-p compile:*",
    "compile:esm": "tsc -b ./config/tsconfig.build.json",
    "compile:cjs": "tsc -b ./config/tsconfig.cjs.json",
    "clean": "run-p clean:*",
    "clean:build": "bux rmrf ./build",
    "clean:esm": "bux rmrf ./lib",
    "clean:cjs": "bux rmrf ./lib-cjs",
    "check-format": "prettier --list-different --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"src/**/*.ts\" \"test/**/*.ts\" \"samples-dev/**/*.ts\" \"*.{js,json}\"",
    "format": "prettier --write --config ../../../.prettierrc.json --ignore-path ../../../.prettierignore \"src/**/*.ts\" \"test/**/*.ts\" \"samples-dev/**/*.ts\" \"*.{js,json}\"",
    "generate:client": "autorest --typescript swagger/README.md && npm run format",
    "lint:fix": "eslint package.json api-extractor.json src test --ext .ts --fix --fix-type [problem,suggestion]",
    "lint": "eslint",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:all": "npm run test:coverage",
    "unit-test": "npm run unit-test:node && npm run unit-test:browser",
    "unit-test:node": "dev-tool run test:node-ts-input -- --timeout 1200000 --exclude 'test/**/browser/*.spec.ts' 'test/**/*.spec.ts'",
    "unit-test:browser": "dev-tool run test:browser"
  },
  "sideEffects": false,
  "autoPublish": false,
  "dependencies": {
    "@azure-rest/core-client": "1.0.0-beta.10",
    "@azure/core-auth": "^1.3.0",
    "@azure/core-lro": "^2.2.0",
    "@azure/core-paging": "^1.2.0",
    "@azure/core-rest-pipeline": "^1.8.0",
    "tslib": "^2.2.0"
  },
  "devDependencies": {
    "@azure/identity": "^2.0.1",
    "@batch/common-config": "file:../../util/common-config",
    "@batch/ui-common": "file:../common",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-json": "^5.0.2",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/chai": "^4.2.8",
    "@types/jest": "^29.2.4",
    "@types/node": "^14.0.0",
    "@types/node-fetch": "^2.6.2",
    "autorest": "latest",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^8.0.0",
    "jest": "^29.3.1",
    "jest-junit": "^12.2.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.7",
    "nyc": "^14.0.0",
    "prettier": "2.2.1",
    "rollup": "^2.66.1",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "source-map-support": "^0.5.9",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "//metadata": {
    "constantPaths": [
      {
        "path": "swagger/README.md",
        "prefix": "package-version"
      }
    ]
  }
}
