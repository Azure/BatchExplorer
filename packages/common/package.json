{
  "name": "@batch/ui-common",
  "private": true,
  "version": "1.0.0",
  "description": "Common library for Azure Batch user interfaces",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/BatchExplorer"
  },
  "author": "Microsoft",
  "license": "MIT",
  "main": "./lib-cjs/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "test": "jest",
  "scripts": {
    "build": "yarn compile",
    "build:clean": "run-s clean compile",
    "build:test": "run-s build test",
    "compile": "run-p compile:*",
    "compile:esm": "tsc -b ./config/tsconfig.build.json",
    "compile:cjs": "tsc -b ./config/tsconfig.cjs.json",
    "clean": "run-p clean:*",
    "clean:build": "butil rmrf ./build",
    "clean:lib": "butil rmrf ./lib",
    "clean:lib-cjs": "butil rmrf ./lib-cjs",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:all": "yarn test:coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "watch": "yarn compile:esm --watch --preserveWatchOutput",
    "workspace:build": "yarn build",
    "workspace:build:clean": "yarn build:clean",
    "workspace:build:desktop": "yarn build",
    "workspace:build:web": "yarn build",
    "workspace:build:package": "yarn build:clean",
    "workspace:build:prod": "yarn build:clean",
    "workspace:build:test": "yarn build:test",
    "workspace:clean": "yarn clean",
    "workspace:launch:desktop": "yarn watch",
    "workspace:launch:web": "yarn watch",
    "workspace:lint": "yarn lint",
    "workspace:lint:fix": "yarn lint --fix",
    "workspace:start:desktop": "yarn watch",
    "workspace:start:web": "yarn watch",
    "workspace:test": "yarn test",
    "workspace:test:all": "yarn test:all",
    "workspace:test:desktop": "yarn test",
    "workspace:test:web": "yarn test",
    "workspace:test:web:all": "yarn test:all"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^2.0.2",
    "lodash": "^4.17.20",
    "luxon": "^2.0.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@batch/build-util": "^1.0.0",
    "@batch/common-config": "^1.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.14.37",
    "jest": "^27.1.0",
    "jest-junit": "^12.2.0",
    "npm-run-all": "^4.1.5"
  },
  "files": [
    "lib",
    "lib-cjs"
  ]
}
