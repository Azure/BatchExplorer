{
  "name": "@batch/build-util",
  "version": "0.0.2",
  "description": "Cross platform CLI containing helpers for NPM tasks",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/BatchExplorer"
  },
  "author": "Microsoft",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./bin/cli.js",
  "bin": {
    "butil": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc -p . && node ./bin/cli.js chmodx ./bin/cli.js",
    "butil": "npm run build && node ./bin/cli.js",
    "clean": "rimraf bin/",
    "lint": "eslint .",
    "lint:fix": "npm run lint --fix",
    "build:clean": "run-s clean build",
    "prepack": "npm run build:clean",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "workspace:test:all": "npm run build:clean && npm test"
  },
  "dependencies": {
    "cli-color": "^2.0.2",
    "editorconfig": "^0.15.3",
    "inquirer": "^8.2.3",
    "shelljs": "^0.8.4",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@types/cli-color": "^2.0.2",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.29",
    "@types/semver": "^7.3.10",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.10",
    "jest": "^28.1.0",
    "jest-junit": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.3",
    "which": "^2.0.2"
  }
}
