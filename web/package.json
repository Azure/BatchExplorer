{
  "name": "@batch/explorer-web",
  "private": true,
  "version": "1.0.0",
  "description": "Test library for use in the Batch portal extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/BatchExplorer"
  },
  "author": "Microsoft",
  "license": "MIT",
  "module": "./lib/index.js",
  "browser": "./lib-umd/batchexplorer.js",
  "types": "./lib/index.d.ts",
  "test": "jest",
  "scripts": {
    "build": "run-s compile bundle:dev",
    "build:clean": "run-s clean compile bundle:dev",
    "build:prod": "run-s clean compile bundle:prod",
    "build:test": "run-s build test",
    "bundle": "yarn bundle:dev",
    "bundle:dev": "webpack --env.dev",
    "bundle:prod": "webpack",
    "butil": "butil",
    "compile": "tsc -b ./config/tsconfig.build.json",
    "clean": "run-p clean:*",
    "clean:build": "butil rmrf ./build",
    "clean:lib": "butil rmrf ./lib",
    "clean:umd": "butil rmrf ./lib-umd",
    "test": "jest",
    "test:coverage": "jest --collect-coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "server": "webpack-dev-server --inline --hot",
    "start": "yarn start:dev",
    "start:dev": "yarn clean && yarn server --env.dev",
    "start:prod": "yarn clean && yarn server",
    "watch": "yarn compile --env.watch",
    "workspace:build": "yarn build",
    "workspace:build:clean": "yarn build:clean",
    "workspace:build:web": "yarn build",
    "workspace:build:package": "yarn build:prod",
    "workspace:build:prod": "yarn build:prod",
    "workspace:build:test": "yarn build:test",
    "workspace:clean": "yarn clean",
    "workspace:launch:web": "yarn start --env.launch",
    "workspace:lint": "yarn lint",
    "workspace:lint:fix": "yarn lint --fix",
    "workspace:start:web": "yarn start",
    "workspace:test": "yarn test",
    "workspace:test:coverage": "yarn test:coverage",
    "workspace:test:web": "yarn test"
  },
  "dependencies": {
    "@batch/ui-common": "^1.0.0",
    "@batch/ui-playground": "^1.0.0",
    "@batch/ui-react": "^1.0.0",
    "@batch/ui-service": "^1.0.0",
    "@fluentui/react": "7.147.1",
    "@fluentui/react-theme-provider": "0.16.2",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@uifabric/azure-themes": "7.5.19",
    "mobx": "^6.3.2",
    "mobx-react-lite": "^3.2.0",
    "monaco-editor": "^0.25.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "^5.2.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@batch/build-util": "^1.0.0",
    "@batch/common-config": "^1.0.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "esbuild-loader": "^2.11.1",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "monaco-editor-webpack-plugin": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-sources": "^1.4.3"
  },
  "files": [
    "lib",
    "lib-umd"
  ]
}
